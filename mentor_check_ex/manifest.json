{
   "name": "MentorCheckEx",
   "background": {
      "scripts": ["js/background.js"]
   },
   "description": "課題レビューのチェックをする拡張機能",
   "icons": {
      "128": "img/icon-128.png",
      "16": "img/icon-16.png",
      "32": "img/icon-32.png",
      "48": "img/icon-48.png"
   },
   "manifest_version": 2,
   "permissions": [
      "https://techacademy.jp/mentor/all/*"
    ],
   "content_scripts": [{
      "matches": ["https://techacademy.jp/mentor/all/*"],
      "run_at": "document_end",
      "js": [
         "js/content.js"
      ],
      "css": [
         "css/style.css"
      ]
   }],
   "options_page": "options.html",
   "web_accessible_resources": [
      "resources/chime.mp3"
    ],
   "version": "0.0.1"
}
