{
   "manifest_version": 3,
   "name": "MentorCheckEx",
   "version": "0.0.1",
   "action": {},
   "background": {
      "service_worker": "background.js"
    },
   "icons": {
      "16": "img/icon-16.png",
      "32": "img/icon-32.png",
      "48": "img/icon-48.png",
      "128": "img/icon-128.png"
   },
   "description": "課題レビューのチェックをする拡張機能",
   "permissions": ["storage", "tabs"],
   "content_scripts": [
      {
         "matches": ["https://techacademy.jp/mentor/all/*"],
         "run_at": "document_end",
         "js": ["js/content.js"],
         "css": ["css/style.css"]
      }
   ],
   "options_page": "options.html",
   "web_accessible_resources": [{
      "resources": ["resources/chime.mp3"],
      "matches": ["https://techacademy.jp/*"]
   }]
}
